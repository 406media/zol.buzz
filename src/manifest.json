{
  "name": "Zol.buzz",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Smart URLs for the cannabis world",
  "permissions": [
    "tabs",
    "https://zol.buzz/"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "page_action": {
    "default_icon": "icon16.png",
    "default_title": "Generate Zol.buzz Short URL",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
